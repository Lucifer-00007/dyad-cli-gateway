# API Configuration
VITE_API_BASE_URL=http://localhost:3001

# Environment
VITE_NODE_ENV=development

# Feature Flags
VITE_ENABLE_DEVTOOLS=true
VITE_ENABLE_STREAMING=true
VITE_ENABLE_BULK_OPERATIONS=true
VITE_ENABLE_ADVANCED_METRICS=true
VITE_ENABLE_EXPORT_FEATURES=true
VITE_ENABLE_CHAT_PLAYGROUND=true

# Security
VITE_CSRF_HEADER_NAME=X-CSRF-Token
VITE_CSRF_COOKIE_NAME=csrf-token

# Cache Configuration
VITE_CACHE_MAX_AGE=86400000
VITE_QUERY_STALE_TIME=300000
VITE_QUERY_CACHE_TIME=600000

# Monitoring & Observability
VITE_ENABLE_ERROR_TRACKING=false
VITE_SENTRY_DSN=
VITE_ENVIRONMENT=development
VITE_APP_VERSION=1.0.0

# Analytics
VITE_ANALYTICS_ENABLED=true
VITE_ANALYTICS_ENDPOINT=

# Logging
VITE_LOG_LEVEL=info
VITE_ENABLE_CONSOLE_LOGS=true
VITE_ENABLE_REMOTE_LOGS=false
VITE_ENABLE_LOG_STORAGE=true
VITE_MAX_LOG_ENTRIES=1000
VITE_LOGGING_ENDPOINT=

# Performance Monitoring
VITE_PERFORMANCE_MONITORING=true
VITE_ENABLE_VIRTUAL_SCROLLING=true
VITE_PAGE_SIZE_DEFAULT=10
VITE_PAGE_SIZE_MAX=100

# Health Checks
VITE_HEALTH_CHECK_INTERVAL=60000
VITE_HEALTH_CHECK_TIMEOUT=5000
VITE_HEALTH_CHECK_RETRIES=3